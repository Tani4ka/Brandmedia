mixin dreams()
  +b.SECTION.dreams
    +e.DIV.wrap
      +e.DIV.content
        +b.DIV.d-content
          +e.SPAN.supname Creative Agency
          +e.H1.heading.h1 We Make Dreams
          +e.P.text Beautiful interfaces and creative solutions

          +b.DIV.d-buttons
            +e.A.first.btn.btn_blue-light(href='#') Read more
            +e.A.last.btn.btn_white(href='#') Portfolio

      +b.DIV.video
